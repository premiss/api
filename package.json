{
	"name": "premiss",
	"version": "1.0.0",
	"description": "TypeScript testing api",
	"main": "index.js",
	"config": {
		"tcr": ""
	},
	"scripts": {
		"test": "npm run lint && npm run build && npm run commit --premiss:tcr=\"$npm_package_config_tcr\" || npm run revert",
		"build": "tsc -p tsconfig.json",
		"lint": "eslint ./src/**/*.ts ./test/**/*.ts",
		"commit": "git add . && git add -u && git commit -m \"TCR $npm_package_config_tcr\"",
		"revert": "git reset --hard"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gotmonkey/premiss.git"
	},
	"keywords": [
		"TypeScript",
		"Test"
	],
	"author": "Blake Lindsay",
	"license": "CC0-1.0",
	"bugs": {
		"url": "https://github.com/gotmonkey/premiss/issues"
	},
	"homepage": "https://github.com/gotmonkey/premiss#readme",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "2.24.0",
		"@typescript-eslint/parser": "2.24.0",
		"eslint": "6.8.0",
		"typescript": "3.8.3"
	}
}